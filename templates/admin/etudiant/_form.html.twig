{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.nom_e) }}</div>
    <div class="col-md-4">{{ form_row(form.prenom_e) }}</div>
    <div class="col-md-4">{{ form_row(form.parain) }}</div>
    <!--<div class="col-md-2">{#{ form_row(form.matieres) }#}</div>-->
</div>
<div class="row">
    <div class="col-md-3">{{ form_row(form.date_naissance_e) }}</div>
    <div class="col-md-3">{{ form_row(form.lieu_naissance_e) }}</div>
    <div class="col-md-2">{{ form_row(form.sexe_e) }}</div>
    <div class="col-md-2">{{ form_row(form.nationalite_e) }}</div>
    <div class="col-md-2">{{ form_row(form.scolaire) }}</div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.imageFile) }}</div>
    <div class="col-md-4">{{ form_row(form.addresse_e) }}</div>
    <div class="col-md-3">{{ form_row(form.phone_e) }}</div>
    <div class="col-md-3">{{ form_row(form.email_e) }}</div>
    <!--<div class="col-md-4">{3{ form_row(form.parain_id) }}</div>-->
</div>
{{ form_widget(form)}}
<div class="text-right"><button class="btn btn-primary" style="color: #FFFFFF;">{{ button|default('Enregister') }}</button></div>
{{ form_end(form) }}